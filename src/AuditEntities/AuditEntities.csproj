<Project Sdk="Microsoft.NET.Sdk">

    <PropertyGroup>
        <TargetFramework>net8.0</TargetFramework>
        <ImplicitUsings>enable</ImplicitUsings>
        <Nullable>enable</Nullable>
        <Authors>Sitename</Authors>
        <Copyright>MIT</Copyright>
        <PackageIcon>Sitename.png</PackageIcon>
        <PackageReadmeFile>Readme.md</PackageReadmeFile>
        <Version>1.0.0</Version>
        <PackageId>Sitename.AuditEntities</PackageId>
        <PackageTags>library, Track Entities, Track Entity Properties</PackageTags>
        <Title>AuditEntities</Title>
        <Description>Retrive essential data from Entity Framework Core's ChangeTracker.</Description>
        <RepositoryUrl></RepositoryUrl>
        <PackageReleaseNotes></PackageReleaseNotes>
        <GeneratePackageOnBuild>False</GeneratePackageOnBuild>
    </PropertyGroup>

	<ItemGroup>
		<PackageReference Include="Microsoft.EntityFrameworkCore.Relational" Version="8.0.4" />
		<PackageReference Include="Microsoft.ApplicationInsights.AspNetCore" Version="2.22.0" />
	</ItemGroup>

    <ItemGroup>
        <None Include="..\..\Readme.md" Pack="true" PackagePath="\" />
    </ItemGroup>

</Project>
